generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model House {
  id          String   @id @default(cuid())
  lat         Float
  lng         Float
  properties  Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

